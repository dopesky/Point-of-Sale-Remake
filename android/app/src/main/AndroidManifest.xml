<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.herokuapp.pointofsale">
	<uses-permission android:name="android.permission.INTERNET" />
	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"

		android:theme="@style/HiddenTitleTheme"
		>
		<activity android:name=".Auth.ForgotPassword"
			android:parentActivityName=".Auth.MainActivity"></activity>
		<activity
			android:name=".Auth.SignUp"
			android:parentActivityName=".Auth.MainActivity" />
		<activity android:name=".Auth.MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
	</application>
</manifest>
